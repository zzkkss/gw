<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gw.model">
	<class
		name="Member"
		table="member"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="Id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Sex"
			column="sex"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Idtype"
			column="idtype"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Idnum"
			column="idnum"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Pushmsg"
			column="pushmsg"
			type="string"
			not-null="false"
			length="255"
		/>


	</class>	
</hibernate-mapping>